<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Colors -->
    <Color x:Key="Dark">#212529</Color>
    <Color x:Key="MediumDark">#24282c</Color>
    <Color x:Key="LightDark">#495057</Color>
    <Color x:Key="Light">#e9ecef</Color>
    
    <!-- Brushes-->
    <SolidColorBrush x:Key="DarkBrush" Color="{StaticResource Dark}"/>
    <SolidColorBrush x:Key="MediumDarkBrush" Color="{StaticResource MediumDark}"/>
    <SolidColorBrush x:Key="LightDarkBrush" Color="{StaticResource LightDark}"></SolidColorBrush>
    <SolidColorBrush x:Key="LightBrush" Color="{StaticResource Light}"></SolidColorBrush>
    
</ResourceDictionary>
